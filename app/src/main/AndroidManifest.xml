<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.studyjams.s2.sj109.ieee745">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DecoderActivity"
            android:label="@string/title_activity_decoder" />
        <activity
            android:name=".EncoderActivity"
            android:label="@string/title_activity_encoder" />
        <activity
            android:name=".DoubleEncoderActivity"
            android:label="@string/title_activity_double_encoder" />
        <activity
            android:name=".DoubleDecoderActivity"
            android:label="@string/title_activity_double_decoder" />
        <activity android:name=".AboutActivity"
            android:label="@string/title_activity_about"/>
    </application>

</manifest>